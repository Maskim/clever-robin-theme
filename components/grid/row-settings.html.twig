<div class="container styleguide_container">
    <label class="styleguide_label">div.container</label>
    <div class="row {% if modifier_class %}{{ modifier_class }}{% endif %} styleguide_row">
        <label class="styleguide_label">div.row{% if modifier_class %}.{{ modifier_class }}{% endif %}</label>
        {% for i in 1..3 %}
            <div class="col-xs-2 styleguide_col" style="min-height: {{ 40 * i }}px">.col-xs-2</div>
        {% endfor %}
    </div>
</div>